mapa_RS <- readRDS("MapaRSMunicipios.RDS")
mapa_RS@data$NOME <- stri_conv(as.character(mapa_RS@data$NOME), "latin1", "UTF-8")
mapa_Cor <- readRDS("MapaRSCoredes.RDS")
